<script>
    // const 대신 var를 쓰거나, window 객체에 담습니다.
    var ctxPath = "${pageContext.request.contextPath}";
    
    // 이미 있으면 새로 만들지 않음
    if (typeof window.ctxPath === "undefined") {
        window.ctxPath = "${pageContext.request.contextPath}";
    }
</script>


