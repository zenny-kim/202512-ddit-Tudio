package kr.or.ddit.faq.service.impl;

import java.util.List;

import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.stereotype.Service;

import kr.or.ddit.faq.mapper.IFaqMapper;
import kr.or.ddit.faq.service.IFaqService;
import kr.or.ddit.vo.FaqVO;
import lombok.extern.slf4j.Slf4j;

@Slf4j
@Service
public class FaqServiceImpl implements IFaqService {
	
	@Autowired
	private IFaqMapper faqMapper;
	
	@Override
	public List<FaqVO> selectFaqList() {
		return faqMapper.selectFaqList();
	}

	
	
}
